                            <path class="circle"
                                  stroke-dasharray="@Model.OtherSpecification.Friendliness_To_Childern" , 100"
                                  d="M18 2.0845
          a 15.9155 15.9155 0 0 1 0 31.831
          a 15.9155 15.9155 0 0 1 0 -31.831" />
                            <text x="18" y="20.35" class="percentage">@Model.OtherSpecification.Friendliness_To_Childern%</text>
