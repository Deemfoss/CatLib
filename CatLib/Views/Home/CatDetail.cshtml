                            <path class="circle"
                                  stroke-dasharray="@Model.OtherSpecification.Vocality" , 100"
                                  d="M18 2.0845
          a 15.9155 15.9155 0 0 1 0 31.831
          a 15.9155 15.9155 0 0 1 0 -31.831" />
                            <text x="18" y="20.35" class="percentage">@Model.OtherSpecification.Vocality%</text>
                        </svg>
                    </div>
                </div>
                <div class="cat-detail-temperaments-container-body">
                    <p>@Model.CompatibilityDescription.Vocality_Description</p>
                </div>
            </div>
            <div class="cat-detail-temperaments-container3">
                <h3 class="cat-detail-temperaments-container-header">Grooming</h3>
                <div class="cat-detail-temperaments-container-procent">
                    <div class="single-chart">
                        <svg viewBox="0 0 36 36" class="circular-chart orange">
                            <path class="circle-bg"
                                  d="M18 2.0845
          a 15.9155 15.9155 0 0 1 0 31.831
          a 15.9155 15.9155 0 0 1 0 -31.831" />
                            <path class="circle"
                                  stroke-dasharray="@Model.OtherSpecification.Grooming" , 100"
                                  d="M18 2.0845
          a 15.9155 15.9155 0 0 1 0 31.831
          a 15.9155 15.9155 0 0 1 0 -31.831" />
                            <text x="18" y="20.35" class="percentage">@Model.OtherSpecification.Grooming%</text>
                        </svg>
                    </div>
                </div>
                <div class="cat-detail-temperaments-container-body">
                    <p>@Model.CompatibilityDescription.Grooming_Description</p>
                </div>
            </div>
        </div>

    </div>
    <div class="cat-detail-navigation-history-container">
        <div class="cat-detail-article-list">
            <img src="~/img/CatsProfile/@Model.OtherSpecification.Kitty_Img"/>
        </div>
        <div class="cat-detail-header-container-history">
            <h2 class="cat-detail-description-name">How to choose a kitten @Model.Name</h2>
            <p class="cat-detail-description-txt">@Model.OtherSpecification.Choosing_Kitten</p>
        </div>
    </div>

    <div class="cat-detail-traits-container">
        <h2 class="cat-detail-description-name">Compability of @Model.Name</h2>
